    %pushi/vec4 0, 0, 4;
    %assign/vec4 v00000213054f9a80_0, 0;
    %load/vec4 v00000213054f9f80_0;
    %inv;
    %assign/vec4 v00000213054f9f80_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v00000213054f9a80_0;
    %addi 1, 0, 4;
    %assign/vec4 v00000213054f9a80_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_00000213054fb2e0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00000213054fa020_0, 0, 1;
